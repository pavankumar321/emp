<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="student" class="com.pavan.spring.Student">
	<property name="id" value="10"/>
	<property name="name" value="Pawana"/>
	<property name="marks">
		<list>
			<value>50</value>
			<value>60</value>
			<value>70</value>
		</list>
	</property>
	<property name="hobbies">
		<list>
			<value>Playing cricket</value>
			<value>Watching Movies</value>
			<value>Watching Movies</value>
		</list>
	</property>
	<property name="subjects">
		<set>
			<value>C</value>
			<value>Java</value>
			<value>OS</value>
			<value>OS</value>
		</set>
	</property>
	<property name="ranks">
		<map>
			<entry key="1" value="pavan"/>
			<entry>
				<key><value>2</value></key>
				<value>ramu</value>
			</entry>
			<entry key="3">
				<value>koti</value>
			</entry>
		</map>
	</property>
	<property name="mobileNos">
		<props>
			<prop key="venu">9848022338</prop>
			<prop key="Mani">9876543210</prop>
		</props>
	</property>

</bean>
</beans>